@page "/unauthorized"
@inject IStringLocalizer<Resource> localizer
@using System.Diagnostics

<PageTitle>@localizer["Unauthorized"]</PageTitle>

<div class="row" style="margin:5px">
    <div class="col">
        <h4>@localizer["Status code 401 - Unauthorized"]</h4>
        <p role="alert">@localizer["You need to login to access this request."]</p>
    </div>
</div>
